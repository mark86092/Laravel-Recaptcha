{
    "name": "mark86092/recaptcha",
    "description": "reCAPTCHA Validator for Laravel 5",
	"keywords": ["recaptcha", "captcha", "laravel", "laravel5"],
	"homepage": "http://github.com/mark86092/recaptcha",
	"license": "MIT",
    "authors": [
        {
            "name": "Yen-chi Chen",
            "email": "mark86092@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~5.2",
        "google/recaptcha": "~1.1",
        "mockery/mockery": "^0.9.4"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*"
	},
    "autoload": {
        "psr-4": {
            "Mark86092\\Recaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mark86092\\Tests\\Recaptcha\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
